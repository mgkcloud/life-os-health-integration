{
  "name": "health-integration-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "concurrently -k \"pnpm dev:server\" \"pnpm dev:metro\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/_core/index.ts",
    "dev:metro": "cross-env EXPO_NO_METRO_WORKSPACE_ROOT=1 npx expo start --web --port ${EXPO_PORT:-8081}",
    "build": "esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "expo lint",
    "format": "prettier --write .",
    "test": "vitest run",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:rebuild": "pnpm prebuild && pnpm ios:pods && pnpm ios",
    "android:rebuild": "pnpm prebuild && pnpm android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "qr": "node scripts/generate_qr.mjs",
    "sendit": "npx eas-cli build -p ios --profile production --auto-submit",
    "app-store:setup": "./scripts/asc-setup.sh",
    "app-store:status": "./scripts/asc-status.sh",
    "app-store:submit": "./scripts/asc-submit.sh",
    "app-store:testflight": "./scripts/asc-testflight.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "^54.0.32",
    "expo-apple-authentication": "~8.0.8",
    "expo-background-fetch": "~14.0.9",
    "expo-device": "~8.0.10",
    "expo-linking": "^8.0.11",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.9",
    "patch-package": "^8.0.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-health": "^1.19.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^19.1.0",
    "typescript": "~5.3.0"
  },
  "private": true
}
